@article{mortonPoliticalImpactCanadian1987,
  title = {The {{Political Impact}} of the {{Canadian Charter}} of {{Rights}} and {{Freedoms}}},
  author = {Morton, F. L.},
  year = {1987},
  journal = {Canadian Journal of Political Science / Revue canadienne de science politique},
  volume = {20},
