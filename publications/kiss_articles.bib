@article{eidlinWhyThereNo2016,
  title = {Why {{Is There No Labor Party}} in the {{United States}}? {{Political Articulation}} and the {{Canadian Comparison}}, 1932 to 1948},
  shorttitle = {Why {{Is There No Labor Party}} in the {{United States}}?},
  author = {Eidlin, Barry},
