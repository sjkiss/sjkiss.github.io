@article{helliwellWesternAccordLower1986,
  title = {The {{Western Accord}} and {{Lower World Oil Prices}}},
  author = {Helliwell, John and MacGregor, Mary and McRae, Robert and Plourde, Andre},
  year = {1986},
  month = may,
  journal = {Canadian Public Policy / Analyse de Politiques},
  volume = {12},
  number = {2},
  pages = {341--355},
  abstract = {L'entente energetique avec les provinces de l'Ouest souligne l'ouverture vers les marches mondiaux des prix et de la production de l'industrie petroliere canadienne. L'entente et les changements subsequents au niveau de la fiscalite et des royautes de chaque palier de gouvernement denotent aussi un important transfert de fonds du gouvernement federal vers les compagnies productrices. L'intention derriere ces deux changements combines, c'etait alors de permettre a l'industrie petroliere d'etre un 'moteur de croissance' pour le reste de l'economie, surtout dans l'Ouest canadien. Dans le present article, nous utilisons le modele MACE pour analyser les effets globaux de ces deux aspects de l'entente avec l'Ouest. Nous analysons aussi les consequences de la baisse radicale des prix mondiaux du petrole au debut de 1986, en examinant les effets macroeconomiques de meme que les effets directs sur les taxes et les royautes sur le petrole, sur les revenus accumules par l'industrie et sur la vitalite de l'industrie petroliere. L'effet de la dereglementation sur les prix, les revenus energetiques et l'activite economique est en somme assez modeste, tandis que les effets fiscaux auront probablement un impact beaucoup plus vaste. Ces changements augmentent legerement l'efficacite de l'extraction, mais reduisent les rendements economiques nets accumules au nom des Canadiens. L'impact macroeconomique direct de changements dans la fiscalite et les royautes sera, en effet, positif; mais on estime que cet impact s'annulera si on doit prelever d'autres taxes pour les fins de l'entente. L'impact des revenus petroliers moins eleves entre 1985 et 1990 a cause de la baisse des prix est absorbe a 40 pour cent par l'industrie, 40 pour cent par les gouvernements des provinces productrices et 20 pour cent par le gouvernement federal. Pourtant, pendant la meme periode, le choc des prix du petrole mene a une economie d'aggregat jouissant d'une croissance plus rapide, d'une inflation et d'un chomage moindres. /// The Western Accord marks the opening up to world markets of the pricing and production of the Canadian petroleum industry. The Accord, and the subsequent tax and royalty changes by both levels of government, also mark a major transfer of revenues from the federal government to the producing firms. The combination of these two changes was intended to make the petroleum industry an 'engine of growth' for the rest of the economy, especially in Western Canada. This paper uses the MACE model to analyze the overall effects of these two aspects of the Western Accord. We also analyse the consequences of the sharp drop of world oil prices in early 1986, examining the macroeconomic effects as well as the direct effects on petroleum taxes and royalties, industry revenues, and the level of activity in the petroleum industry. The net effect of deregulation on prices, energy revenues, and economic activity is fairly modest, while the tax changes are likely to have much larger effects. They slightly increase the efficiency of extraction, but reduce net economic rents accruing to Canadians. The direct macroeconomic impacts of the tax and royalty changes will indeed be positive, but these effects are estimated to be more than offset if other taxes are raised to pay for the Accord. The impact of lower petroleum revenues over the 1985-1990 period due to the low oil prices is absorbed about 40 per cent by industry, 40 per cent by the governments of the producing provinces, and 20 per cent by the federal government. However, during the same period, the oil price shock leads to an aggregate economy that is growing faster with less inflation and less unemployment.},
  keywords = {nosource}
}

