@article{laforestTraditionalEcologicalKnowledge2018,
  title = {Traditional {{Ecological Knowledge}} of {{Polar Bears}} in the {{Northern Eeyou Marine Region}}, {{Qu{\'e}bec}}, {{Canada}}},
  author = {Laforest, Brandon J. and H{\'e}bert, Julie S. and Obbard, Martyn E. and Thiemann, Gregory W.},
