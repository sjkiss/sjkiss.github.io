@article{bakvisQuebecBlockVoting1995,
  title = {Quebec {{Block Voting}} and the {{Canadian Electoral System}}},
  author = {Bakvis, Herman and Macpherson, Laura G.},
  year = {1995},
  month = dec,
  journal = {Canadian Journal of Political Science/Revue canadienne de science politique},
  volume = {28},
  number = {4},
  pages = {659--692},
  issn = {1744-9324, 0008-4239},
  doi = {10.1017/S000842390001934X},
  urldate = {2024-08-23},
  abstract = {This article examines two contrasting views of Quebec block voting in federal elections. One view has it that the Quebec electorate tends to behave in a monolithic fashion; the other, put by Cairns in 1968, claims that this phenomenon is ``a contrivance of the electoral system, not an autonomous social fact.'' The evidence shows that block voting, namely, the degree to which an electorate mobilizes behind the largest party, is much more pronounced in Quebec than it is in other provinces, and especially when compared to that in Ontario. Quebec is also more likely than Ontario to determine which party forms the government. The tendency of the electoral system to magnify seat allocations to the largest party, however, is only marginally greater in Quebec relative to Ontario. At the same time, the composition of the Quebec ``block'' vote has varied considerably over time; it has not always been a distinctively francophone phenomenon. The article further challenges the assumption that the single-member plurality system is inherently inferior to proportional representation as a means of securing the protection of minorities in ethnically diverse societies., Cet article considere deux points de vue diff{\'e}rents {\`a} propos du vote en bloc au Qu{\'e}bec lors des {\'e}lections f{\'e}d{\'e}rales. L'un de ces points de vue est que l'{\'e}lectorat qu{\'e}b{\'e}cois a tendance {\`a} se comporter de mani{\`e}re monolithique; le second point de vue, que Cairns a exprim{\'e} en 1968, avance que ce ph{\'e}nom{\`e}ne est <<une cr{\'e}ation du syst{\`e}me {\'e}lectoral, pas un fait social autonome>>. Les statistiques r{\'e}v{\`e}lent que le vote en bloc, c'est-{\`a}-dire le degr{\'e} auquel l'{\'e}lectorat se mobilise pour appuyer le plus grand parti, est un ph{\'e}nom{\`e}ne beaucoup plus marqu{\'e} au Qu{\'e}bec que dans les autres provinces, et en particulier en comparaison avec l'Ontario. D'autre part, le Qu{\'e}bec est sans doute plus en mesure de d{\'e}terminer quel parti va constituer le gouvernement que l'Ontario. La tendance du syst{\`e}me {\'e}lectoral a grossir les r{\'e}partitions des si{\`e}ges au parti le plus important, n'est toutefois que l{\'e}g{\`e}rement plus accentu{\'e}e au Qu{\'e}bec qu'en Ontario. Simultan{\'e}ment, la composition du vote en bloc du Qu{\'e}bec a consid{\'e}rablement vari{\'e} au cours du temps; cela n'a pas toujours {\'e}t{\'e} un ph{\'e}nom{\`e}ne francophone distinct. En outre, l'article remet en question l'hypoth{\`e}se selon laquelle le syst{\`e}me de circonscription uninominale {\`a} scrutin majoritaire est un moins bon moyen que le syst{\`e}me d'{\'e}lection {\`a} la proportionnelle pour assurer la protection des minorit{\'e}s dans des soci{\'e}t{\'e}s compos{\'e}es d'ethnies diverses.},
  langid = {english}
}

@article{bakvisRegionalPoliticsPolicy1989,
  title = {Regional {{Politics}} and {{Policy}} in the {{Mulroney Cabinet}}, 1984-88: {{Towards}} a {{Theory}} of the {{Regional Minister System}} in {{Canada}}},
  shorttitle = {Regional {{Politics}} and {{Policy}} in the {{Mulroney Cabinet}}, 1984-88},
  author = {Bakvis, Herman},
  year = {1989},
  journal = {Canadian Public Policy / Analyse de Politiques},
  volume = {15},
  number = {2},
  pages = {121--134},
  publisher = {[University of Toronto Press, Canadian Public Policy]},
  issn = {0317-0861},
  doi = {10.2307/3551157},
  urldate = {2024-06-25},
  abstract = {On pr{\'e}tend que le dernier cabinet Trudeau (1980-84) a connu un renversement de la tendance {\`a} la baisse du pouvoir des ministres r{\'e}gionaux. Cet article affirme que ce renversement n'{\'e}tait ni temporaire, ni unique. En effet, non seulement le syst{\`e}me des ministres r{\'e}gionaux a jou{\'e} un r{\^o}le important au sein du cabinet Mulroney (1981-88), mais sa force et son degr{\'e} de persistance ont probablement {\'e}t{\'e} sous-{\'e}valu{\'e}s. La forme sp{\'e}cifique de l'influence des ministres r{\'e}gionaux sous Mulroney, soit l'apparition d'un nombre limit{\'e} d'individus ayant une influence importante sur les projets majeurs, y est {\'e}tudi{\'e}e. Pour expliquer le r{\^o}le des ministres r{\'e}gionaux au sein des cabinets Trudeau et Mulroney, on met l'accent sur deux facteurs - la complexit{\'e} administrative de l'{\'e}tat moderne et l'apparition d'un f{\'e}d{\'e}ralisme comp{\'e}titif - qui, ironiquement, sont les facteurs habituellement invoqu{\'e}s pour expliquer le d{\'e}clin du pouvoir des ministres r{\'e}gionaux. /// It has been claimed that the last Trudeau cabinet (1980-84) saw a reversal in the trend towards the decline of regional ministers. This article argues that this reversal was neither temporary nor unique. Not only has the regional minister system been evident in the policies and operations of the Mulroney cabinet (1984-88) but its overall strength and persistence has probably been undervalued. The specific form of regional ministerial influence under Mulroney, namely the emergence of a limited number of regional figures exercising significant influence over major projects, is examined. To account for the role of regional ministers in both the Trudeau and Mulroney cabinets, attention is drawn to two factors - the complexities of the modern administrative state and the rise of modern competitive federalism - ironically the same factors usually cited in connection with the decline of regional ministers.},
  file = {/Users/skiss/Zotero/storage/B9BRJDGL/Bakvis - 1989 - Regional Politics and Policy in the Mulroney Cabinet, 1984-88 Towards a Theory of the Regional Mini.pdf}
}

