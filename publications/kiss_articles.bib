  author = {Stump, Donald G and Beck, Melissa J and Radovsky, Ann and Garman, Robert H and Freshwater, Lester L and Sheets, Larry P and Marty, M Sue and Waechter, John M and Dimond, Stephen S and Van Miller, John P and Shiotsuka, Ronald N and Beyer, Dieter and Chappelle, Anne H and Hentges, Steven G},
