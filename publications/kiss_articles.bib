@article{venkatakrishnanMicrosomalBindingAmitriptyline2000,
  title = {Microsomal Binding of Amitriptyline: {{Effect}} on Estimation of Enzyme Kinetic Parameters in Vitro},
  author = {Venkatakrishnan, K and {von Moltke}, L and Obach, R and Greenblatt, D},
  year = {2000},
  month = apr,
  journal = {Journal of Pharmacology and Experimental Therapeutics},
  volume = {293},
  number = {2},
  pages = {343--350},
  abstract = {The effect of binding of amitriptyline to human liver microsomes and to microsomes from human B-lymphoblastoid cells on the estimation of enzyme kinetic parameters describing N-demethylation to nortriptyline was investigated using a combination of microsomal binding and in vitro enzyme kinetic studies. Quantitative binding in both matrices increased with higher microsomal protein concentrations (free fractions 0.88-0.32 at 100-500 mu g protein/ml in human liver microsomes and 0.82-0.26 at 250-1000 mu g protein/ml in microsomes from B-lymphoblastoid cells) and was independent of amitriptyline concentration over a concentration range of 0.2 to 200 mu M. Addition of heat-inactivated microsomal protein (50-450 mu g/ml) to native human liver microsomes (50 mu g/ml) reduced the amitriptyline N-demethylation rate in a protein concentration dependent manner. This effect was greater at lower substrate concentrations and was overcome by saturating concentrations of substrate, thereby decreasing the apparent affinities of the high- and low-affinity components of the N-demethylation process, with minimal effect on the net V-max. Addition of metabolically inactive microsomes from untransfected human lymphoblastoid cells (750 mu g/ml) to CYP2C19 (250 mu g/ml protein) increased the apparent K-m value for amitriptyline N-demethylation by 3.5-fold and increased the uncompetitive substrate inhibition constant (K-s) by 2.2-fold, making substrate inhibition essentially undetectable. A similar effect was seen with CYP3A4, with a 1.8-fold increase in the S-50 (substrate concentration at which half-maximal velocity of a Hill enzyme is achieved). Microsomal binding did not alter the V-max of either CYP isoform to any appreciable extent. These findings emphasize the importance of incorporating microsomal binding in the estimation of enzyme kinetic parameters in vitro and making appropriate corrections for unbound drug concentrations.},
  keywords = {nosource}
}

