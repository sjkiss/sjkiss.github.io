  doi = {10.2139/ssrn.4128267},
  urldate = {2023-12-13},
  abstract = {The 2018 Ontario election was an historic wipeout for the provincial Liberals. Conceding defeat a week in advance of Election Day, then-premier Kathleen Wynne saw the electorate move against her, with her unpopularity sinking any chances of taking her Liberal party to another election victory. While the rise of populism and the ``Ford Nation'' phenomenon may have propelled voters to elect the Progressive Conservatives, there is also the possibility that the Liberal loss was a result of an aggressive social media campaign that targeted Wynne, personally. Studies have revealed the ability of Web 2.0 and algorithmic personalization of content to affect attitudes and behaviour. In this paper, we take a closer look at this phenomenon by examining whether engagement with social media platforms, such as Facebook and Twitter, explains negative views towards Wynne. We contrast this with exposure to more conventional media (newspaper, television, radio) to determine if Wynne's unpopularity was more general (i.e., fatigue towards the Liberals), or if it was more uniquely driven by social media.},
  langid = {english},
  keywords = {elections,memes,Ontario,social media},
  file = {/Users/skiss/Zotero/storage/M7A8AQT6/Rana and Perrella - 2021 - Death by Meme Social Media’s Role in Kathleen Wyn.pdf}
}

