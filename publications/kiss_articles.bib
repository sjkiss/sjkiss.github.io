@article{dyckCanadaAssistancePlan1976,
  title = {The {{Canada Assistance Plan}}: The Ultimate in Cooperative Federalism},
  shorttitle = {The {{Canada Assistance Plan}}},
  author = {Dyck, Rand},
  year = {1976},
  journal = {Canadian Public Administration},
  volume = {19},
