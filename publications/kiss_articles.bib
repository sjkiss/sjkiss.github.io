@article{margolisRacemicFluoxetineNdemethylation2000,
  title = {({{R}})-, ({{S}})-, and Racemic Fluoxetine {{N-demethylation}} by Human Cytochrome {{P450}} Enzymes},
  author = {Margolis, J and O'Donnell, J and Mankowski, D and Ekins, S and Obach, R},
  year = {2000},
