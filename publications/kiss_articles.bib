@article{gidronPoliticsSocialStatus2017,
  title = {The Politics of Social Status: Economic and Cultural Roots of the Populist Right},
  shorttitle = {The Politics of Social Status},
  author = {Gidron, Noam and Hall, Peter A.},
  year = {2017},
  journal = {The British Journal of Sociology},
  volume = {68},
